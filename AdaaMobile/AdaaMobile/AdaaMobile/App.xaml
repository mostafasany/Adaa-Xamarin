<Application
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="AdaaMobile.App">
    <Application.Resources>
        <ResourceDictionary>
            <Style x:Key="labelStyle" TargetType="Label">
                <Setter Property="TextColor" Value="Green" />
            </Style>
          <!--Update Value-->
          <Color x:Key="AppBackgroundDark" >#FF007C84</Color>
         
          <Color x:Key="AppBackgroundLight" >#FF007C84</Color>
          <Color x:Key="YellowAccent" >#FFFFD205</Color>
        </ResourceDictionary>
    </Application.Resources>
</Application>